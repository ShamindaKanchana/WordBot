{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)\\.(?:css|js|json|png|jpg|gif|ico|svg)$",
      "dest": "/static/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}